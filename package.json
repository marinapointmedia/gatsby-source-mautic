    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "chokidar": "^1.7.0",
    "cross-env": "^5.0.5",
    "danger": "^3.4.7"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marinapointmedia/gatsby-source-mautic.git"
  },
  "bugs": {
    "url": "https://github.com/marinapointmedia/gatsby-source-mautic/issues"
