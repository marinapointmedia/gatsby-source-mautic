{
  "name": "@marinapointmediainc/gatsby-source-mautic",
  "version": "0.8.0",
  "description": "Source plugin to provide Mautic API endpoints in Gatsby Fork of gatsby-source-mautic",
  "author": "marinapointmedia <contact@marinapointmedia.com>",
  "license": "GPL-3.0",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "source",
    "mautic"
